{
  "name": "struct.com.au",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "develop": "ENABLE_GATSBY_REFRESH_ENDPOINT=true NODE_NO_WARNINGS=1 gatsby develop -S",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "test": "jest",
    "test:watch": "jest --watch",
    "serve": "gatsby serve",
    "storybook": "start-storybook -p 6006",
    "clean": "gatsby clean",
    "build": "gatsby build",
    "build:storybook": "build-storybook",
    "update": "ncu -u",
    "storyblok:login": "storyblok login",
    "storyblok:logout": "storyblok logout",
    "storyblok:pull": "storyblok pull-components --space=176295 && yarn storyblok:types",
    "storyblok:push": "storyblok push-components --space=176295 ./components.176295.json",
    "storyblok:types": "storyblok-generate-ts source=./components.176295.json target=./src/@types/storyblok"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@storyblok/storyblok-editable": "^1.1.0",
