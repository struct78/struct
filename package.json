    "emotion-reset": "^3.0.1",
    "gatsby": "^4.25.6",
    "gatsby-omni-font-loader": "^2.0.2",
    "gatsby-plugin-emotion": "^7.25.0",
    "gatsby-plugin-image": "^2.25.0",
    "gatsby-plugin-manifest": "^4.25.0",
    "gatsby-plugin-offline": "^5.25.0",
    "gatsby-plugin-postcss": "^5.25.0",
    "gatsby-plugin-preconnect": "^1.4.0",
    "gatsby-plugin-react-axe": "^0.5.0",
    "gatsby-plugin-react-helmet": "^5.25.0",
    "gatsby-plugin-sharp": "^4.25.1",
    "gatsby-plugin-tailwindcss": "^1.0.3",
    "gatsby-plugin-typescript": "^4.25.0",
    "gatsby-source-filesystem": "^4.25.0",
    "gatsby-source-storyblok": "^4.2.1",
    "gatsby-storyblok-image": "^0.3.0",
    "gatsby-transformer-sharp": "^4.25.0",
    "graphql-request": "^5.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hamburger-menu": "^1.2.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.8.0",
    "react-lazyload": "^3.2.0",
    "react-scrolllock": "^5.0.1",
    "storyblok-react": "^0.1.2",
    "storyblok-rich-text-react-renderer": "^2.6.1",
    "tailwind-vertical-rhythm": "^1.2.0",
    "tailwindcss": "^3.3.1",
    "twin.macro": "^3.3.0"
  },
  "devDependencies": {
    "@netlify/plugin-a11y": "^1.0.0-beta.1",
    "@netlify/plugin-lighthouse": "^4.0.7",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addons": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.5.0",
    "@types/loadable__component": "^5.13.4",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "@types/react-hamburger-menu": "0.0.4",
    "@types/react-helmet": "^6.1.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.30",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-styled-components": "^7.1.1",
    "lint-staged": "^13.2.0",
    "npm-check-updates": "^16.9.0",
    "postcss": "^8.4.21",
    "pretty-quick": "^3.1.3",
    "storyblok": "^3.20.0",
    "storyblok-generate-ts": "^1.13.2",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "babelMacros": {
    "twin": {
      "config": "./tailwind.default.js",
      "preset": "emotion"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
